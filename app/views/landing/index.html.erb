            stop_graph();
            query();
            $('body').css({'height':'100%'});
            $('#form').ScrollTo({
                duration: 2000, offsetTop: 13, offsetLeft: 0,
                callback: function () {
                    $('body').css({'overflow-y': 'scroll'});
                }
            });
        }, 6000);
    }

    function query() {
        $.ajax({
            type: "GET",
            url: "/query.js",
            data: { query_string: "Filip" }
        });
    }


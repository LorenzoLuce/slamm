                    s,
                    {
                        x: prefix + 'x',
                        y: prefix + 'y',
                        color: prefix + 'color',
                        size: prefix + 'size'
                    }
            );
        }, 2000);
    }
    function stop_graph(){
        clearInterval(graph_anima);
